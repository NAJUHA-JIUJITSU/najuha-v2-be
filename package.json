{
  "name": "najuha-v2-be",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "------------------------BUILDS------------------------": "",
    "build": "nest build",
    "build:sdk": "NODE_ENV=dev npx nestia sdk",
    "build:swagger": "NODE_ENV=dev npx nestia swagger",
    "build:erd": "NODE_ENV=dev node dist/src/generate-erd.js",
    "build:docs": "NODE_ENV=dev npm run build:swagger && npm run build:erd",
    "------------------------STARTS------------------------": "",
    "start": "nest start",
    "start:dev": "NODE_ENV=dev nest start --watch",
    "start:performance": "NODE_ENV=performance nest start --watch",
    "start:prod": "NODE_ENV=prod node dist/src/main",
    "start:debug": "nest start --debug --watch",
    "------------------------TESTS------------------------": "",
    "test": "jest --config ./test/jest.json --detectOpenHandles",
    "test:docker": "make test && jest && make test-fclean",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:integration": "jest --config ./test/jest-integration.json",
    "test:e2e": "jest --config ./test/jest-e2e.json --detectOpenHandles",
    "test:e2e-make": "make test && jest --config ./test/jest-e2e.json --detectOpenHandles; make test-fclean",
    "------------------------ETC------------------------": "",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "prepare": "ts-patch install && typia patch"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.592.0",
    "@aws-sdk/s3-presigned-post": "^3.592.0",
    "@nestia/core": "^3.2.1",
    "@nestia/e2e": "^0.4.3",
    "@nestjs/axios": "^3.0.1",
    "@nestjs/common": "^10.3.0",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^10.3.0",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/swagger": "^7.1.17",
    "@nestjs/typeorm": "^10.0.1",
    "axios": "^1.6.3",
    "cache-manager-ioredis": "^2.1.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "cli-progress": "^3.12.0",
    "cross-env": "^7.0.3",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "multer-s3": "^3.0.1",
    "nest-winston": "^1.9.4",
    "pg": "^8.11.3",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "sqlite3": "^5.1.7",
    "swagger-ui-express": "^5.0.0",
    "typeorm": "^0.3.17",
    "typia": "^6.0.3",
    "uuidv7": "^1.0.0",
    "winston": "^3.11.0",
    "winston-daily-rotate-file": "^4.7.1"
  },
  "devDependencies": {
    "@nestia/sdk": "^2.4.3",
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/cli-progress": "^3.11.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.2",
    "@types/multer-s3": "^3.0.3",
    "@types/node": "^20.3.1",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "eslint": "^8.56.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.5.0",
    "nestia": "^5.3.0",
    "prettier": "^3.0.0",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.2",
    "ts-patch": "^3.1.1",
    "tsconfig-paths": "^4.2.0",
    "typeorm-markdown-generator": "^1.0.5",
    "typescript": "^5.3.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "modulePaths": [
      "<rootDir>/.."
    ],
    "testRegex": ".*\\.(spec|test)\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "moduleNameMapper": {
      "^src/(.*)$": "<rootDir>/$1"
    }
  }
}
